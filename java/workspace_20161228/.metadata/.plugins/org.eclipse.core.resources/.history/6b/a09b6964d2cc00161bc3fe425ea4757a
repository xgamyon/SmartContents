package 연습문제;

import java.util.Scanner;

public class TestMain {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int x;
		int y = 1000;
		int z;
		System.out.print("meter 입력 : ");
		x = sc.nextInt();
		
		z = x/y;

		System.out.printf("값 : %dKm",z);
		System.out.printf(" %dm\n",x%y);
	}

}
