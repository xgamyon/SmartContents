package 연습문제;

import java.util.Scanner;

public class TestMain2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int km, m, result;
		
		System.out.print("km 입력 : ");
		km = sc.nextint();
		
		System.out.print("m 입력 : ");
		m = sc.nextInt();
		
		result = km*1000+m;
		
		System.out.printf("결과값은 %dm 입니다.", result);
	}

}
