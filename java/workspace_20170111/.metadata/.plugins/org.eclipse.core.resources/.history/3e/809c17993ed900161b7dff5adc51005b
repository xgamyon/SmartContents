<!-- 파일명 : receiveArray.jsp -->
<%
// 배열의 경우에는 request.getParameterValues로 받아야 한다.
String []sports = request.getParameterValues("sports");
String result="";
for(int i=0; i<sports.length; i++){
	if(sports[i].equals9\("1")){
		result = result + "야구";
	}
}
%>
