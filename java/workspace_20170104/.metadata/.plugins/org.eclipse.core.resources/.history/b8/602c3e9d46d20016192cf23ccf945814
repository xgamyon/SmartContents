package 객체지향프로그램2;

public class Person {
	private String name;
	private int age;
	
	// 마우스 우클릭 Source - Genarate Getters and Setters - 전부 체크 
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	
	
}
