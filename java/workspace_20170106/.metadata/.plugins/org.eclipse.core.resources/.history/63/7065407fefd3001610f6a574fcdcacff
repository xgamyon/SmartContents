
public class TestMain {
	public static void main(String[] args) {
		
		StringBuilder sb = new StringBuilder(); //문자를 계속 덧붙일때 쓴다.
		String s = new String();
		int i;
		
		System.out.println("시작");
		for(i=1; i<=100; i++){
			s = s+i;
		}
		System.out.println(s);
		
		System.out.println("시작");
		for(i=1; i<=100; i++){
			sb.append(i);
		}
		System.out.println(sb);
	}
}
